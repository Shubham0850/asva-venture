nst currentLink = encodeURIComponent(window.location.href);\n    setCurrentLink(currentLink);\n  }, []);\n  \n  const handleTwitterShare = () => {\n    const tweetText = \"Check out this link: \" + currentLink;\n    const twitterShareUrl = `https://twitter.com/intent/tweet?text=${tweetText}`;\n\n    // Open the Twitter share intent in a new window\n    window.open(twitterShareUrl, \"_blank\");\n  };\n\n  const handleLinkedInShare = () => {\n    const linkedInShareUrl = `https://www.linkedin.com/shareArticle/?mini=true&url=${currentLink}`;\n\n    // Open the LinkedIn share intent in a new window\n    window.open(linkedInShareUrl, \"_blank\");\n  };\n\n  const copyToClipboard = () => {\n    const currentLink = window.location.href;\n\n    navigator.clipboard.writeText(currentLink).then(\n      () => {\n        // If the copy was successful, you can show a success message or perform any other action\n        alert(\"Link copied to clipboard!\");\n      },\n      (err) => {\n        // If there was an error copying the link, you can handle it here\n        console.error(\"Could not copy link to clipboard:\", err);\n      }\n    );\n  };\n\n  return (\n    <>\n      {loading ? (\n        <p>Loading...</p>\n      ) : (\n        data && (\n          <Box pt={\"105px\"} bg={\"#fff\"}>\n            <Head>\n              <title>{data.title}</title>\n              <meta\n                name=\"description\"\n                content={`${parse(data?.content as string)}`}\n              />\n            </Head>\n            <Box\n              py={30}\n              borderBottom={\"1px solid #ddd\"}\n              borderStyle={\"dashed\"}\n            >\n              <Container maxW={1300} p=\"0px 25px\">\n                <Box w={{ base: \"100%\", md: \"80%\" }}>\n                  <Text\n                    //  fontFamily={\"PowerGrotesk\"}\n                    mb={\"18px\"}\n                    fontWeight={700}\n                    fontSize={{ base: \"36px\", md: \"64px\" }}\n                    color={\"#000\"}\n                    lineHeight={{ base: \"40px\", md: \"70px\" }}\n                  >\n                    {data.title}\n                  </Text>\n\n                  <Flex gap={4} mt={\"25px\"} wrap={\"wrap\"}>\n                    {Object.keys(data?.tags).map((tag, index) => {\n                      return (\n                        <Tag\n                          key={index}\n                          size={\"sm\"}\n                          variant=\"solid\"\n                          background=\"#6EFE96\"\n                          color={\"#1F1F1F\"}\n                          rounded={\"full\"}\n                          padding={\"2px 12px\"}\n                        >\n                          {tag}\n                        </Tag>\n                      );\n                    })}\n                  </Flex>\n                  <Text\n                    color={\"#7A7A7A\"}\n                    fontSize={\"20px\"}\n                    fontWeight={500}\n                    py={4}\n                  >\n                    {`${data?.author?.first_name} ${data?.author?.last_name}`}\n                  </Text>\n                </Box>\n              </Container>\n            </Box>\n            <Container maxW={1300} py={20} p=\"0px 25px\">\n              <Box pt=\"40px\">\n                <Flex\n                  justifyContent={\"space-between\"}\n                  flexDir={{ base: \"column\", md: \"row\" }}\n                >\n                  <Text mb={{ base: 5, md: 0 }}>{dateFormate(data.date)}</Text>\n                  <Flex alignItems={\"center\"} gap={3}>\n                    <Button\n                      color={\"#4C545A\"}\n                      border={\"0.5px solid #4C545A\"}\n                      rounded={\"full\"}\n                      gap={2}\n                      onClick={() => {\n                        copyToClipboard();\n                      }}\n                    >\n                      <FiCopy style={{ fontSize: 20 }} />\n                      <Text fontSize={14}>Copy link</Text>\n                    </Button>\n\n                    <Button\n                      onClick={handleTwitterShare}\n                      border={\"0.5px solid #4C545A\"}\n                      rounded={\"full\"}\n                    >\n                      <BsTwitter style={{ color: \"#4C545A\", fontSize: 20 }} />\n                    </Button>\n                    <Button\n                      onClick={handleLinkedInShare}\n                      border={\"0.5px solid #4C545A\"}\n                      rounded={\"full\"}\n                    >\n                      <BsLinkedin style={{ color: \"#4C545A\", fontSize: 20 }} />\n                    </Button>\n                  </Flex>\n                </Flex>\n              </Box>\n\n              <Box\n                px={{ base: \"16px\", md: \"32px\" }}\n                py={8}\n                w={{ base: \"100%\", md: \"80%\" }}\n                mx=\"auto\"\n              >\n                <Image\n                  src={data.featured_image}\n                  maxH={\"500px\"}\n                  width={\"100%\"}\n                  objectFit={\"cover\"}\n                  mb={\"25px\"}\n                />\n                <div className=\"content-box\">\n                  <Text fontSize={{ base: \"16px\", md: \"20px\", color: \"#888\" }}>\n                    {parse(data?.content as string)}\n                  </Text>\n                </div>\n              </Box>\n            </Container>\n            <Subscribe />\n          </Box>\n        )\n      )}\n    </>\n  );\n}\n\nexport default Blog;\n","/Users/omkar/Documents/workspace/asva-venture/src/pages/index.tsx",[],[],"/Users/omkar/Documents/workspace/asva-venture/src/pages/podcast/[podcast].tsx",[],[],"/Users/omkar/Documents/workspace/asva-venture/src/pages/recent-post.tsx",[],[],"/Users/omkar/Documents/workspace/asva-venture/src/pages/research.tsx",[],[],"/Users/omkar/Documents